    "def get_min_numbers(numbers):\n",
    "    result = numbers[0]\n",
    "    for num in numbers:\n",
    "        if result > num:\n",
    "            result = num\n",
    "    return result\n",
    "\n",
    "#Gọi hàm:\n",
    "list1 = [30, 29, 10]\n",
    "\n",
    "min_number = get_min_numbers(list1)\n",
    "print(f\"Min number: {min_number}\")"
   ]
  }
