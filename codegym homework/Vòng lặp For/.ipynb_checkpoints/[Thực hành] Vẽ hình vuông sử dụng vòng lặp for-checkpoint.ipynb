    "import turtle\n",
    "\n",
    "wn = turtle.Screen()\n",
    "\n",
    "# cài đặt cửa sổ\n",
    "wn.bgcolor(\"black\")\n",
    "wn.title(\"Square\")\n",
    "\n",
    "# cài đặt bút vẽ hình vuông\n",
    "\n",
    "t = turtle.Turtle()\n",
    "t.speed(0)\n",
    "t.color(\"#FF0000\")\n",
    "\n",
    "# tạo vòng lặp để vẽ hình\n",
    "for i in range(4):\n",
    "    t.forward(100)\n",
    "    t.left(90)\n",
    "    \n",
    "turtle.done()"
   ]
  }
